{
  "name": "arqlopes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cf:build": "npx opennextjs-cloudflare build",
    "cf:preview": "npx opennextjs-cloudflare build && npx wrangler pages dev .open-next",
    "cf:deploy": "npx opennextjs-cloudflare build && npx wrangler pages deploy .open-next --project-name=arqlopes-site",
    "pages:build": "npx opennextjs-cloudflare build && mv .open-next/worker.js .open-next/_worker.js && ls -la .open-next/assets/ && cp -r .open-next/assets/. .open-next/ && ls -la .open-next/ && rm -rf .open-next/assets",
    "r2:seed": "node scripts/seed-r2.mjs"
  },
  "dependencies": {
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.5.1",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "input-otp": "1.4.1",
    "lucide-react": "^0.454.0",
    "next": "15.2.8",
    "next-themes": "^0.4.4",
    "react": "^19",
    "react-dom": "^19",
    "react-fast-marquee": "^1.6.5",
    "react-resizable-panels": "^2.1.7",
    "resend": "^5.0.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.6"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.14.8",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "postcss": "^8.5",
    "tailwindcss": "^4.0.0",
    "typescript": "^5",
    "wrangler": "^4.58.0"
  }
}
